<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="15dp">

    <TextView
        android:id="@+id/tv_data_intent_move_fragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="hello ini DataIntentMoveFragment"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_data_intent_move_fragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="isi disini"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_data_intent_move_fragment" />

    <Button
        android:id="@+id/btn_data_intent_move_fragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="button data intent move fragment"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/et_data_intent_move_fragment" />

</androidx.constraintlayout.widget.ConstraintLayout>